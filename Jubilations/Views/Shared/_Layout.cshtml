<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Jubilations</title>
    <link href="~/Design/Header/Header.css" rel="stylesheet" />
    <link href="~/Design/Footer/Footer.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>







    <header class="fixed-top" style="background-color: #2D4262;">

        <div class="header-main ">
            <div class="container">
                <nav id='cssmenu'>
                    <div class="logo">
                        <img class="img" src="~/image/Logo/icon.png" style="height:40px; width:auto; " />
                        <img class="img" src="~/image/Logo/jubilations.png" style="height:40px; width:auto; " />
                    </div>
                    <div id="head-mobile"></div>
                    <div class="button"></div>
                    <ul>
                        <li><a href='#'>Home</a></li>
                        <li>
                            <a href='#'>Services</a>
                            <ul>
                                <li><a href='#'>Services 1</a></li>
                                <li>
                                    <a href='#'>Services 2</a>
                                    <ul>
                                        <li><a href='#'>Services 21</a></li>
                                    </ul>
                                </li>

                                <li><a href='#'>Services 3</a></li>
                                <li><a href='#'>Services 4</a></li>
                            </ul>
                        </li>
                        <li><a href='#'>Reviews</a></li>
                        <li>@Html.ActionLink("Blog", "Blog1", "Contact", new { area = "" }, new { @class = "nav-link" })</li>
                        <li>@Html.ActionLink("About", "About_us", "Contact", new { area = "" }, new { @class = "nav-link" })</li>
                        <li>@Html.ActionLink("Contact", "Contact_us", "Contact", new { area = "" }, new { @class = "nav-link" })</li>
                        <li>@Html.ActionLink("Login/SingUp", "Login", "Login", new { area = "" }, new { @class = "nav-link" })</li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>




    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>






    <footer class="footer-section">
        <div class="container">
            <div class="footer-cta pt-5 pb-5">
                <div class="row">
                    <div class="col-xl-4 col-md-4 mb-30">
                        <div class="single-cta">
                            <i class="fas fa-map-marker-alt"></i>
                            <div class="cta-text">
                                <h4>Find us</h4>
                                <span>1010 Avenue, sw 54321, chandigarh</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-4 mb-30">
                        <div class="single-cta">
                            <i class=""></i>
                            <div class="cta-text">
                                <h4>Call us</h4>
                                <span>9876543210 0</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-4 mb-30">
                        <div class="single-cta">
                            <i class=""></i>
                            <div class="cta-text">
                                <h4>Mail us</h4>
                                <span>mail@info.com</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-content pt-5 pb-5">
                <div class="row">
                    <div class="col-xl-4 col-lg-4 mb-50">
                        <div class="footer-widget">
                            <div class="footer-logo">
                                <img class="img" src="~/image/Logo/icon.png" style="height:40px; width:auto; " />
                                <img class="img" src="~/image/Logo/jubilations.png" style="height:40px; width:auto; " />
                            </div>

                            <div class="footer-social-icon">
                                <span>Follow us On</span>
                                <ul class="footer-social">
