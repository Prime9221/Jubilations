
@{
    ViewBag.Title = "FNewPass";
}

<h2>FNewPass</h2>

<link href="~/Design/Forgotpass/Fpass.css" rel="stylesheet" />


<style>
    body {
        background-image: url('../../image/background/bg-0.jpg');
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-size: 100% 100%;
    }
</style>


<body>
    <div class="form">
        <div class="form__box">
            <div class="form__left">
                <div class="form__padding"><img class="form__image" src="~/image/forgote_password/new_pass-cover.png" /></div>
            </div>
            <div class="form__right">
                <div class="form__padding-right">
                    <form>
                        
                        <h1 class="form__title"> Enter New Password</h1>
                        <input class="form__email" type="text" placeholder="Enter Password" />
                        <input class="form__email" type="text" placeholder="Confirm Password" />
                        <button id="Prime">@Html.ActionLink("Done", "Login", "Login", new { @id = "Prime2" })</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</body>